extends layout 

block content 
    h1= title
    ul
        each val, index in character 
            if(typeof val !='function')
                li #{index}, #{val}
        //- li Image: #{character.image}
        li= character.description
        li= character.category
        p= character
    //- p= response
    //- ul
    //-     each val, index in image
    //-         if(typeof val !='function')
    //-             li #{index}
